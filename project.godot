; Engine configuration file.
; It's best edited using the editor UI.

config_version=5

[application]
config/name="DynamicRPG"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus", "GL Compatibility")

[display]
window/size/viewport_width=1280
window/size/viewport_height=720

[mono]
project/assembly_name="DynamicRPG"
project/target_framework="net6.0"
project/load_scripts_on_each_save=true

[rendering]
renderer/rendering_method="forward_plus"
